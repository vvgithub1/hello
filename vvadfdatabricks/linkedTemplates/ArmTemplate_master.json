{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vvadfdatabricks"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"ls_ablob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob'"
		},
		"ls_adls_covidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdl'"
		},
		"ls_formula1dl_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_formula1dl_storage'"
		},
		"ls_adls_covidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vvstoragelake2.dfs.core.windows.net"
		},
		"ls_formula1dl_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vvstoragelake2.dfs.core.windows.net"
		},
		"tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"trigger1_properties_test_parameters_parent_parameters_p_file_date": {
			"type": "string",
			"defaultValue": "@trigger().startTime"
		},
		"event_trigger_population_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/8292c435-c459-400b-bb35-7762e5d549d6/resourceGroups/rg_storage/providers/Microsoft.Storage/storageAccounts/vvstoragedata"
		},
		"ls_covid_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"trigger2_properties_pl_ingest_ecdc_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "github.com/cloudboxacademy/covid19/raw/main/ecdc_data/cases_deaths.csv"
		},
		"trigger2_properties_pl_ingest_ecdc_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "cases_deaths.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vvadfdatabricks_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"ls_ablob_connectionString": {
						"value": "[parameters('ls_ablob_connectionString')]"
					},
					"ls_adls_covidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdl_accountKey')]"
					},
					"ls_formula1dl_storage_accountKey": {
						"value": "[parameters('ls_formula1dl_storage_accountKey')]"
					},
					"ls_adls_covidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdl_properties_typeProperties_url')]"
					},
					"ls_formula1dl_storage_properties_typeProperties_url": {
						"value": "[parameters('ls_formula1dl_storage_properties_typeProperties_url')]"
					},
					"ls_covid_source_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_source_properties_typeProperties_url')]"
					},
					"trigger1_properties_test_parameters_parent_parameters_p_file_date": {
						"value": "[parameters('trigger1_properties_test_parameters_parent_parameters_p_file_date')]"
					},
					"event_trigger_population_properties_typeProperties_scope": {
						"value": "[parameters('event_trigger_population_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "vvadfdatabricks_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger2_properties_pl_ingest_ecdc_parameters_sourceRelativeURL": {
						"value": "[parameters('trigger2_properties_pl_ingest_ecdc_parameters_sourceRelativeURL')]"
					},
					"trigger2_properties_pl_ingest_ecdc_parameters_sinkFileName": {
						"value": "[parameters('trigger2_properties_pl_ingest_ecdc_parameters_sinkFileName')]"
					},
					"tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date": {
						"value": "[parameters('tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'vvadfdatabricks_ArmTemplate_0')]"
			]
		}
	]
}