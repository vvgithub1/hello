									"name": "file name",
									"type": "SetVariable",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"variableName": "v_sinkFileName",
										"value": {
											"value": "@item().sinkFileName",
											"type": "Expression"
										}
