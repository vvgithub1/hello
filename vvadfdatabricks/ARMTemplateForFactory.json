										{
											"activity": "base url",
											"dependencyConditions": [
												"Succeeded"
											]
										},
										{
											"activity": "file name",
											"dependencyConditions": [
												"Succeeded"
											]
										},
										{
											"activity": "relative url",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"userProperties": [],
									"typeProperties": {
										"waitTimeInSeconds": 10
									}
								}
							]
						}
					}
				],
				"concurrency": 1,
				"variables": {
					"v_sourceRelativeURL": {
						"type": "String"
					},
					"v_sinkFileName": {
						"type": "String"
					},
					"v_baseURL": {
						"type": "String"
					}
